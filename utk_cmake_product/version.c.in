@CMAKE_DEFINE@ @TARGET_IDENTIFIER@_SEPARATE_INFO_FUNCTIONS_DEFINITIONS


#if defined (${TARGET_IDENTIFIER}_SEPARATE_INFO_FUNCTIONS_DEFINITIONS)
#include "${TARGET_IDENTIFIER}_version.h"
#endif // ${TARGET_IDENTIFIER}_SEPARATE_INFO_FUNCTIONS_DEFINITIONS


#if defined (${TARGET_IDENTIFIER}_LANG_IS_CXX) && \
	defined (${TARGET_IDENTIFIER}_USE_NAMESPACE)
${TARGET_BEGIN_NAMESPACE}
#endif // defined (${TARGET_IDENTIFIER}_LANG_IS_CXX)


unsigned int ${TARGET_FUNCTION_NAME_PREFIX}VersionMajor () {
	return ${TARGET_VERSION_MAJOR};
}


unsigned int ${TARGET_FUNCTION_NAME_PREFIX}VersionMinor () {
	return ${TARGET_VERSION_MINOR};
}


unsigned int ${TARGET_FUNCTION_NAME_PREFIX}VersionPatch () {
	return ${TARGET_VERSION_PATCH};
}


const char* ${TARGET_FUNCTION_NAME_PREFIX}VersionFullString () {
	return
		"${TARGET_VERSION_MAJOR}."
		"${TARGET_VERSION_MINOR}."
		"${TARGET_VERSION_PATCH}"
#ifdef ${TARGET_IDENTIFIER}_HAS_GIT_INFO
		"+${TARGET_GIT_DESCRIBE}"
		"${TARGET_GIT_UNTRACKED}"
#endif // ${TARGET_IDENTIFIER}_HAS_GIT_INFO
		;
}


#ifdef ${TARGET_IDENTIFIER}_HAS_CONTRIBUTORS
const char* ${TARGET_FUNCTION_NAME_PREFIX}Contributors () {
	return ${TARGET_CONTRIBUTORS};
}
#endif // ${TARGET_IDENTIFIER}_HAS_CONTRIBUTORS


#ifdef ${TARGET_IDENTIFIER}_HAS_COPYRIGHT_INFO
const char* ${TARGET_FUNCTION_NAME_PREFIX}CopyrightInfo () {
	return ${TARGET_COPYRIGHT_INFO};
}
#endif // ${TARGET_IDENTIFIER}_HAS_COPYRIGHT_INFO


#ifdef ${TARGET_IDENTIFIER}_HAS_GIT_INFO
const char* ${TARGET_FUNCTION_NAME_PREFIX}GitInfo () {
	return
		"${TARGET_GIT_DESCRIBE}"
		"${TARGET_GIT_UNTRACKED}";
}
#endif // ${TARGET_IDENTIFIER}_HAS_GIT_INFO


#ifdef ${TARGET_IDENTIFIER}_HAS_LICENSE
const char* ${TARGET_FUNCTION_NAME_PREFIX}LicenseName () {
	return "${TARGET_LICENSE_NAME}";
}


const char* ${TARGET_FUNCTION_NAME_PREFIX}License () {
	return
${TARGET_LICENSE};
}
#endif // ${TARGET_IDENTIFIER}_HAS_LICENSE


#ifdef ${TARGET_IDENTIFIER}_HAS_NOTICE
const char* ${TARGET_FUNCTION_NAME_PREFIX}Notice () {
	return ${TARGET_NOTICE};
}
#endif // ${TARGET_IDENTIFIER}_HAS_NOTICE


#if defined (${TARGET_IDENTIFIER}_LANG_IS_CXX) && \
	defined (${TARGET_IDENTIFIER}_USE_NAMESPACE)
${TARGET_END_NAMESPACE}
#endif // defined (${TARGET_IDENTIFIER}_LANG_IS_CXX)
